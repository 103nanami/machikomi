<container>
  <div class="row">
    <div class="col-lg-12 mx-auto">
      <h1>新規投稿</h1>
      <%= form_with model: @column, url:admin_columns_confirm_path, method: :GET, local:true do |f| %>
        <label>タイトル</label>
        <%= f.text_field :title %>

        <label>街</label>

        <label>記事</label>
        <%= f.text_area :text %>

        <label>画像</label>
        <%= f.attachment_field :image %>

        <%= f.submit '確認する' %>
      <% end %>
    </div>
  </div>
</container>
